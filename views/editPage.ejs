      <body id="body">
          <div class="container">
            <h2>Edit your section </h2>
            <form action="/update" method="POST">
              <div class="row100">
                <div class="col">
                  <div class="inputBox">
                     <input type="hidden" name="id" required="required" value="<%= auteur.id %>">
   
                  </div>
                </div>   
             </div>   
            <div class="row100">
               <div class="col">
                 <div class="inputBox">
                    <input type="text" name="name" required="required" value="<%= auteur.name %>">
                    <span class="text">Name</span>
                    <span class="line"></span>
                 </div>
               </div>   
            </div>   
            <div class="row100">
              <div class="col">
                <div class="inputBox">
                   <input type="text" name="biographique" required="required" value="<%= auteur.biographique %>">
                   <span class="text">biographique</span>
                   <span class="line"></span>
                </div>
              </div>   
           </div>   
            <div class="row100"> 
              <div class="col">
                <div class="inputBox textarea">
                  <input type="text" required="required" value="<%= auteur.description %>" name="description"></input> 
                  <span class="text">your citation</span>
                  <span class="line"></span>
                </div>
              </div> 
            </div>   
            <div class="row100">
              <div class="col">
                <div class="inputBox">
                   <input  name="img" required="required" value="<%= auteur.city%>" class="file">
                   <span class="text">city</span>
                   <span class="line"></span>
                </div>
              </div>   
           </div>  
           <div class="row100">
            <div class="col">
              <div class="inputBox">
                 <input  name="img" required="required" value="<%= auteur.books%>" class="file">
                 <span class="text">books</span>
                 <span class="line"></span>
              </div>
            </div>   
         </div>  
            
            <div class="row100"> 
              <div class="col">
                 <input class="btn" type="submit" value="Send">
              </div>
            </div>           
            </div>
          </form>
          </div>

